      \field{month}{11}
      \field{note}{Society for Neuroscience}
      \field{title}{\usebibmacro{bpl:linked}{Neural correlates of value-driven attentional capture in addiction}\usebibmacro{bpl:linked}{}}
      \field{type}{Nanosymposium Session}
